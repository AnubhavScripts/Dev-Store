{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build/client",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "app/entry.server.tsx": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app/entry.server.tsx"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}